{
  "name": "doix-http",
  "version": "1.0.1",
  "description": "HTTP binding for doix",
  "main": "index.js",
  "files": ["/lib", "/badges"],
  "scripts": {
    "test": "jest",
    "test-ci": "jest --ci --coverage"
  },
  "jest": {
	"collectCoverageFrom": [
		"lib/**/*.js"
	],
	"coverageReporters": ["clover", "json", "lcov", "text", "json-summary"],
  	"verbose": false,
  	"testPathIgnorePatterns": [
  		"/node_modules/",
  		"/__tests__/data",
  		"/__tests__/lib"
  	]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/do-/node-doix-http.git"
  },
  "author": "Dmitry Ovsyanko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/do-/node-doix-http/issues"
  },
  "homepage": "https://github.com/do-/node-doix-http#readme",
  "peerDependencies": {
    "doix": "^1.0.5"
  },
  "devDependencies": {
    "jest": "^29.3.1"
  },
  "dependencies": {
    "mime-db": "^1.52.0"
  }
}
